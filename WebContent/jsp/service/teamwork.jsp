#teamwork   div div {
	width: 90%;
	height: auto;
	margin-left: auto;
	margin-right: auto;
	display: table;
}

#teamwork   div div div {
	margin-top: 10px;
	margin-bottom: 10px;
	padding: 0px;
	font-size: 18px;
	font-family: "Microsoft YaHei";
}
